/* task 1.9 */
#include <stdio.h>

int main()
{
	double a, b, arth, harm;
	scanf("%lf %lf", &a, &b);
	arth = (a+b)/2.0f;
	harm = 2.0f/((1.0f/a)+(1.0f/b));
	printf("В арифметичному форматі\n Арифметичне = %lf\n Гармонійне = %lf\n\n\n", arth, harm);
	printf("В науковому форматі\n Арифметичне = %e\n Гармонійне = %e", arth, harm);
	return 0;
}
